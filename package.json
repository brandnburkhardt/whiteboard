{
  "name": "whiteboard",
  "version": "0.0.1",
  "description": "A Desktop Meal Planner and Organizer",
  "main": "build",
  "electron": {
    "name": "Whiteboard"
  },
  "scripts": {
    "start": "run-s dev",
    "test-mocha": "mocha",
    "rebuild": "electron-rebuild -f",
    "start-prod": "electron .",
    "start-dev": "cross-env BABEL_ENV=electron NODE_ENV=development electron -r babel-register app/main",
    "dev": "run-p dev-server start-dev",
    "dev-server": "webpack-dev-server",
    "build": "run-p build-main build-renderer",
    "build-main": "cross-env BABEL_ENV=electron-build webpack --env.platform=electron --env.prod",
    "build-renderer": "cross-env BABEL_ENV=build webpack --env.prod",
    "package": "node tasks/package",
    "package-all": "run-s package -- --all"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "browserslist": [
    "Electron 1.6"
  ],
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^6.24.1",
    "babili-webpack-plugin": "^0.0.11",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "devtron": "^1.2.1",
    "electron-rebuild": "^1.5.11",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "how-the-test-was-won": "^1.0.28",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "node-loader": "^0.6.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "stylelint": "^7.10.1",
    "stylelint-config-css-modules": "^1.0.0",
    "stylelint-config-standard": "^16.0.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "cx-olive-style-core": "git+ssh://git@bitbucket.org/crosschx/cx-olive-style-core.git#1.0.4",
    "cx-redux-utils": "git+ssh://git@bitbucket.org/crosschx/cx-redux-utils.git#master",
    "electron": "1.6.7",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^8.7.0",
    "flexbox-react": "^4.3.3",
    "minimist": "^1.2.0",
    "mocha": "^3.4.1",
    "prop-types": "^15.5.10",
    "ramda": "^0.23.0",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^15.5.4",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^5.0.4",
    "react-router5": "^4.2.1",
    "react-toolbox": "^2.0.0-beta.8",
    "redux": "^3.6.0",
    "redux-effects": "^0.4.3",
    "redux-effects-fetch": "^0.5.5",
    "redux-router5": "^4.2.3",
    "redux-thunk": "^2.2.0",
    "router5": "^4.5.2",
    "stylelint-scss": "^1.4.4",
    "uuid": "^3.0.1"
  }
}
